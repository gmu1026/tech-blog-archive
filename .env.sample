# PostgreSQL
POSTGRES_DB=devbloghub
POSTGRES_USER=devbloguser
POSTGRES_PASSWORD=change-this-password
POSTGRES_PORT=5432

# Redis
REDIS_PORT=6379

# Elasticsearch
ELASTICSEARCH_PORT=9200

# Kibana
KIBANA_PORT=5601

# Kafka
KAFKA_PORT=9092

# Application
APP_PORT=8080

# JWT Secret
JWT_SECRET=replace-with-your-own-secret-key

# Email Configuration
MAIL_HOST=smtp.gmail.com
MAIL_PORT=587
MAIL_USERNAME=your-email@gmail.com
MAIL_PASSWORD=your-app-password

# Firebase Configuration
FIREBASE_PROJECT_ID=your-firebase-project-id
FIREBASE_PRIVATE_KEY_PATH=/app/firebase-private-key.json

# Crawler Configuration
CRAWLER_SCHEDULE=0 0 * * * *  # 매시간 실행 (크론 표현식)
CRAWLER_USER_AGENT=Mozilla/5.0 (compatible; DevBlogHub/1.0; +https://your-domain.com)